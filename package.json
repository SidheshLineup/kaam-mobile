    "setStagingEnvFile": "ENVFILE=.env.staging",
    "setQAEnvFile": "ENVFILE=.env.qa",
    "setProductionEnvFile": "ENVFILE=.env.production",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
